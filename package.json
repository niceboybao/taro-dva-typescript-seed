{
  "name": "taro-dva-typescript-seed",
  "version": "0.0.1",
  "private": true,
  "description": "永享贷小程序种子工程",
  "templateInfo": {
    "name": "redux",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/**/*.*",
    "eslint-fix": "./node_modules/.bin/eslint src/**/*.* --fix",
    "build:weapp-sit": "cross-env SERVER_ENV=sit NODE_ENV=development taro build --type weapp",
    "build:weapp-prod": "cross-env SERVER_ENV=prod NODE_ENV=production taro build --type weapp",
    "build:h5-sit": "cross-env SERVER_ENV=sit NODE_ENV=development taro build --type h5",
    "build:h5-prod": "cross-env SERVER_ENV=prod NODE_ENV=production taro build --type h5",
    "start:weapp-sit": "cross-env SERVER_ENV=sit npm run build:weapp-sit -- --watch",
    "start:h5-sit": "cross-env USE_ANALYZER=No SERVER_ENV=sit npm run build:h5-sit -- --watch",
    "pub:h5-sit": "cross-env USE_ANALYZER=No npm run build:h5-sit && deploy-test",
    "preview-sit": "npm run build:weapp-sit && node deploy/wx.preview.js sit",
    "preview-prod": "npm run build:weapp-prod && node deploy/wx.preview.js prod",
    "upload-sit": "npm run build:weapp-sit && node deploy/wx.upload.js sit",
    "upload-prod": "npm run build:weapp-prod && node deploy/wx.upload.js prod"
  },
  "repository": {
    "type": "git",
    "url": "http://mtgitlab.hnzycfc.com/perpetual/taro-dva-typescript-seed.git"
  },
  "keywords": [
    "taro",
    "dva",
    "typescript",
    "redux",
    "sass"
  ],
  "author": "baldwin",
  "license": "ISC",
  "dependencies": {
    "@sentry/browser": "5.20.1",
    "@tarojs/components": "2.2.9",
    "@tarojs/components-qa": "2.2.9",
    "@tarojs/redux": "2.2.9",
    "@tarojs/redux-h5": "2.2.9",
    "@tarojs/router": "2.2.9",
    "@tarojs/taro": "2.2.9",
    "@tarojs/taro-alipay": "2.2.9",
    "@tarojs/taro-h5": "2.2.9",
    "@tarojs/taro-qq": "2.2.9",
    "@tarojs/taro-quickapp": "2.2.9",
    "@tarojs/taro-swan": "2.2.9",
    "@tarojs/taro-tt": "2.2.9",
    "@tarojs/taro-weapp": "2.2.9",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.6",
    "dva-core": "^2.0.2",
    "dva-loading": "^3.0.20",
    "nerv-devtools": "^1.5.5",
    "nervjs": "^1.5.5",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "0.11.1"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.12.1",
    "@tarojs/cli": "2.2.9",
    "@tarojs/mini-runner": "2.2.9",
    "@tarojs/plugin-sass": "2.2.9",
    "@tarojs/plugin-terser": "2.2.9",
    "@tarojs/webpack-runner": "2.2.9",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-taro": "2.2.9",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-taro": "2.2.9",
    "miniprogram-ci": "^1.0.72",
    "prettier": "^2.0.5",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "2.2.9",
    "stylelint-taro-rn": "2.2.9",
    "typescript": "^3.9.5",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
